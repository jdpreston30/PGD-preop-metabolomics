@article{sumnerProposedMinimumReporting2007,
  title = {Proposed Minimum Reporting Standards for Chemical Analysis: {{Chemical Analysis Working Group}} ({{CAWG}}) {{Metabolomics Standards Initiative}} ({{MSI}})},
  shorttitle = {Proposed Minimum Reporting Standards for Chemical Analysis},
  author = {Sumner, Lloyd W. and Amberg, Alexander and Barrett, Dave and Beale, Michael H. and Beger, Richard and Daykin, Clare A. and Fan, Teresa W.-M. and Fiehn, Oliver and Goodacre, Royston and Griffin, Julian L. and Hankemeier, Thomas and Hardy, Nigel and Harnly, James and Higashi, Richard and Kopka, Joachim and Lane, Andrew N. and Lindon, John C. and Marriott, Philip and Nicholls, Andrew W. and Reily, Michael D. and Thaden, John J. and Viant, Mark R.},
