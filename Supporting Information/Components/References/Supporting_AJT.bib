  shorttitle = {{{MetaboAnalyst}} 6.0},
  author = {Pang, Zhiqiang and Lu, Yao and Zhou, Guangyan and Hui, Fiona and Xu, Lei and Viau, Charles and Spigelman, Aliya F. and MacDonald, Patrick E. and Wishart, David S. and Li, Shuzhao and Xia, Jianguo},
  year = 2024,
  month = jul,
  journal = {Nucleic Acids Research},
  volume = {52},
